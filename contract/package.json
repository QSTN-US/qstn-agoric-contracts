    "eslint-plugin-prettier": "^5.1.3",
    "import-meta-resolve": "^2.2.1",
    "prettier": "^3.0.3",
    "prettier-plugin-jsdoc": "^1.0.0",
    "rollup": "^4.18.0",
    "starshipjs": "^2.4.0",
