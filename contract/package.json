    "fs-extra": "^11.2.0",
    "@agoric/async-flow": "^0.1.1-u17.1",
    "@agoric/cosmic-proto": "^0.5.0-u21.0",
    "@agoric/client-utils": "^0.2.0-u20.0",
    "@agoric/deploy-script-support": "^0.10.4-u17.1",
    "@agoric/eslint-config": "^0.4.1-u16.0",
    "@agoric/kmarshal": "^0.2.0-u22.1",
