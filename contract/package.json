    "@agoric/store": "^0.9.3-u17.1",
    "@agoric/swing-store": "0.10.0-u22.2",
    "@agoric/swingset-vat": "dev",
    "@agoric/telemetry": "dev",
    "@agoric/vats": "^0.16.0-u17.1",
    "@agoric/zoe": "^0.26.3-u17.1",
    "@endo/base64": "^1.0.9",
