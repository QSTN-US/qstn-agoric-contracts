{
  "name": "dapp-evm",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start:ui": "cd ui && yarn dev",
    "lint": "prettier . --check",
    "lint:fix": "prettier . --write",
    "test": "yarn workspace deploy test && yarn workspace solidity test",
    "lint:types": "yarn workspace deploy lint:types && yarn workspace contract lint:types && yarn workspace solidity lint:types",
    "build:solidity": "cd solidity && yarn build",
    "build:deploy": "cd deploy && yarn build",
    "build": "yarn build:solidity && yarn build:local-env",
    "deploy:qstnRouter": "cd deploy && yarn deploy",
    "deploy:contracts": "yarn deploy:qstnRouter",
    "postinstall": "patch-package"
  },
  "private": true,
  "useWorkspaces": true,
  "workspaces": [
    "contract",
    "deploy",
    "solidity"
  ],
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "printWidth": 80
  },
  "resolutions": {
    "@agoric/vats": "^0.16.0-u21.0",
    "@agoric/zoe": "^0.27.0-u21.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
  "devDependencies": {
    "@agoric/eslint-config": "^0.4.1-u11wf.0",
    "concurrently": "^9.1.2",
    "patch-package": "^8.0.0",
    "prettier": "^3.5.3"
  }
}
